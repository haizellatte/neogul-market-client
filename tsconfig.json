{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/Input/Input.tsz",
    "src/app/(providers)/(root)/deals/[dealId]/page.tsx",
    ".next/types/app/(providers)/(root)/my/deals/page.tsx",
    "src/types/k",
    "src/components/SelectTab/SelectTab.tsx"
  ],
  "exclude": ["node_modules"]
}
